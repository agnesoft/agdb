{
    "autoload": {
        "classmap": [
            "src",
            "api"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^11",
        "phpstan/phpstan": "^1.11",
        "jane-php/open-api-3": "^7.8",
        "friendsofphp/php-cs-fixer": "^3.61"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "jane-php/open-api-runtime": "^7.8",
        "symfony/http-client": "^7.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": "true"
        }
    }
}
