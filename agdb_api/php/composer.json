{
    "version": "0.7.2",
    "description": "Agnesoft Graph Database API client for PHP",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "https://github.com/agnesoft/agdb",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Agnesoft",
            "homepage": "https://github.com/agnesoft/agdb"
        }
    ],
    "autoload": {
        "classmap": [
            "src",
            "lib"
        ]
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "phpstan/phpstan": "^1.11"
    }
}
